<?xml version="1.0" encoding="UTF-8"?>

<project name="testuree" basedir=".">
	<property name="make.cmd" value="make"/>
	<target name="testDureeCompile" description="cppunittestcompile">
	  <exec executable="${make.cmd}" failonerror="true"/>
	</target>
	<target name="testDuree" description="cppunittestrun" depends="testDureeCompile">
	  <exec executable="./testDuree" failonerror="false"/>
	</target>
</project>
